{
  "name": "serp-tracker-backend",
  "version": "2.0.0",
  "description": "Professional SERP keyword tracking API with multiple SerpApi key support and automatic failover",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run copy-files",
    "copy-files": "cp package.json dist/ && cp .env.example dist/",
    "typecheck": "tsc --noEmit",
    "db:seed": "ts-node scripts/seedDatabase.ts",
    "healthcheck": "curl -f http://localhost:5000/health || exit 1"
  },
  "keywords": [
    "serp",
    "seo",
    "keyword-tracking",
    "serpapi",
    "ranking",
    "search-engine",
    "api",
    "nodejs",
    "typescript",
    "mongodb"
  ],
  "author": "SERP Tracker Team",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/serp-tracker-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/serp-tracker-backend/issues"
  },
  "homepage": "https://github.com/your-username/serp-tracker-backend#readme"
}
